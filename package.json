{
  "name": "octoshelf",
  "version": "0.0.1",
  "description": "Manage your pull requests with style!",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "lint": "eslint public/scripts/",
    "coverage": "nyc --reporter=html ava tests/",
    "report": "nyc report",
    "check-coverage": "nyc check-coverage --lines 40 --functions 36 --branches 11",
    "test": "npm run coverage; npm run report; npm run check-coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctoShelf/octoshelf.git"
  },
  "keywords": [
    "Github",
    "PullRequest"
  ],
  "author": "Tyler Stark <TheIronDeveloper@gmail.com> (http://www.theirondeveloper.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OctoShelf/octoshelf/issues"
  },
  "homepage": "https://github.com/OctoShelf/octoshelf#readme",
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.0",
    "eslint-config-google": "^0.5.0",
    "jsdom": "^9.2.1",
    "nyc": "^6.4.4",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "ejs": "^2.4.2",
    "es6-promise": "^3.2.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "request": "^2.72.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "whatwg-fetch": "^1.0.0",
    "worker-loader": "^0.7.0"
  },
  "nyc": {
    "exclude": [
      "tests",
      "**/animations.js"
    ]
  }
}
